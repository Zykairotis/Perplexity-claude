      "type": "object",
      "properties": {
        "graphql_query": {
          "type": "string",
          "description": "GraphQL query string"
        }
      },
      "required": [
        "graphql_query"
      ],
      "additionalProperties": false,
      "$schema": "http://json-schema.org/draft-07/schema#"
    }

